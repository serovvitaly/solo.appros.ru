<table id="main-table" class="table table-bordered table-hover table-striped">
  <thead>
    <tr>
      <th>#</th>
      <th>Заголовок</th>
      <th>Метро</th>
      <th>Цена</th>
    </tr>
  </thead>
  <tbody>
  {% for item in items %}
    <tr id="item-{{ item.uid }}">
      <td>{{ item.uid }}</td>
      <td>{{ item.atitle }}</td>
      <td>{{ item.metro }}</td>
      <td>{{ item.price }}</td>
    </tr>
  {% endfor %}
  </tbody>
</table>